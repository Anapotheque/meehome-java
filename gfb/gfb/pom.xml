  <profiles>
    <profile>
      <id>all</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <modules>
        <module>gfb-sinistre-service</module>
        <module>gfb-sinistre-service-client</module>
        <module>gfb-ws-webapp</module>
        <module>gfb-ws-java-client</module>
        <module>gfb-services</module>
        <module>gfb-config</module>
        <module>gfb-ear</module>
        <module>gfb-monitoring</module>
      </modules>
    </profile>
    <profile>
      <id>flex</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <modules>
        <module>gfb-ws-flex-client</module>
      </modules>
    </profile>
    <profile>
      <id>sinistre</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <modules>
        <module>gfb-sinistre-service</module>
        <module>gfb-sinistre-service-client</module>
      </modules>
    </profile>
    <profile>
      <id>peclipse</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <modules>
        <module>gfb-sinistre-service</module>
