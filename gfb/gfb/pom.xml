  <groupId>fr.generali.gfb</groupId>
  <artifactId>gfb</artifactId>
  <packaging>pom</packaging>
  <version>1.1.19-SNAPSHOT</version>
  <name>gfb</name>

  <scm>
    <connection>scm:svn:http://svn.groupe.generali.fr/projets/gfb/trunk</connection>
    <url>http://fisheye.groupe.generali.fr/browse/gfb</url>
