      <dependency>
        <groupId>fr.generali.espaceclient.proxy</groupId>
        <artifactId>generic-proxy</artifactId>
        <version>${eci.version}</version>
      </dependency>

      <dependency>
        <groupId>fr.generali.espaceclient.proxy</groupId>
        <artifactId>rce-proxy</artifactId>
        <version>${eci.version}</version>
      </dependency>
	  
	  
	  
      <dependency>
        <groupId>fr.generali.gfb</groupId>
        <artifactId>gfb-services</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>fr.generali.gfb</groupId>
        <artifactId>gfb-ws-java-client</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>fr.generali.gfb</groupId>
