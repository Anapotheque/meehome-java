        <module>gfb-ws-webapp</module>
        <module>gfb-ws-java-client</module>
        <module>gfb-services</module>
      </modules>
    </profile>
    <profile>
      <id>java</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <modules>
        <module>gfb-sinistre-service</module>
        <module>gfb-sinistre-service-client</module>
        <module>gfb-ws-webapp</module>
        <module>gfb-ws-java-client</module>
        <module>gfb-services</module>
        <module>gfb-config</module>
        <module>gfb-ear</module>
        <module>gfb-monitoring</module>
      </modules>
    </profile>
  </profiles>

  <properties>
    <blazeds.version>3.2.0.3978</blazeds.version>
    <eci.version>11.0</eci.version>
	<eci.eai-client.version>17.2</eci.eai-client.version>
    <elisa.wsclient.version>05.05.02-RC1</elisa.wsclient.version>
    <xfire.version>1.2.7-generali-3</xfire.version>
    <spring.version>2.5.6</spring.version>
